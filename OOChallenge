class vehicle{
  constructor(make, model, year){
    this.make = make; 
    this.model = model; 
    this.year = year;
  }
  
honk(){
 return "beep"
}


toString() {
  return `this vehicle is a ${this.make}`
}


class car extends vehicle(
  constructor (make, model, year) {
    super(make, model, year)
    this.numWheels = 4;
  }
)

class motorcycle extends vehicle{
  constructor (make, model, year) {
  super(make, model, year)
  this.numWheels = 2;
}

revEngine() {
  return "VROOM!!!";
}



